<div class="page-header">
  <h1>Crash Report <small>{{product}} v{{version}}</small></h1>
</div>
<dl class="dl-horizontal">
  {{#eachInMap fields}}
    <dt>{{titleCase @key}}</dt>
    <dd>
    {{#if this.path}}
    <a href="{{this.path}}">download</a>
    {{else}}
    {{this}}
    {{/if}}
    </dd>
  {{/eachInMap}}
</dl>
{{#eachInMap fields}}
{{#if this.preview}}
<h2>{{titleCase @key}}</h2>
<pre><code>
{{this.preview}}
</code></pre>
{{/if}}
{{/eachInMap}}
<h2>Stackwalk</h2>
<pre><code>
{{stackwalk}}
</code></pre>
