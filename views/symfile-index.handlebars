<div class="page-header">
  <h1>Symfiles</h1>
</div>
{{#if records}}
<table class="table">

  <tr>
    <th>Contents</th>
    {{#each fields}}
      <th>{{titleCase this}}</th>
    {{/each}}
  </tr>

  {{#each records}}
    <tr>
      <td>
        <a href="{{symfileUrl this.id}}">view</a>
      </td>
    {{#eachInMap this.props}}
      <td>
        {{this}}
      </td>
    {{/eachInMap}}
    </tr>
  {{/each}}
</table>
{{> pagination}}
{{else}}
  <p>No symfiles</p>
{{/if}}

